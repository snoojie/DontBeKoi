{
    "name": "dontbekoi",
    "engines": {
        "node": "16.13.1"
    },
    "scripts": {
        "dev": "npx ts-node-dev --respawn src/index.ts",
        "devnowatch": "npx ts-node-dev src/index.ts",
        "prod": "rm -rf build/ && npx tsc && node -r source-map-support/register build/index.js",
        "test": "jest && rm -rf coverage/",
        "serverrestart": "heroku ps:restart worker",
        "serveroff": "heroku ps:scale worker=0",
        "serveron": "heroku ps:scale worker=1",
        "serverlogs": "heroku logs --tail"
    },
    "dependencies": {
        "@discordjs/rest": "0.2.0-canary.0",
        "discord.js": "13.6.0",
        "dotenv": "14.1.1",
        "typescript": "4.5.4"
    },
    "devDependencies": {
        "@types/jest": "27.4.0",
        "@types/node": "17.0.9",
        "jest": "27.4.7",
        "source-map-support": "0.5.21",
        "ts-jest": "27.1.3",
        "ts-node": "10.4.0",
        "ts-node-dev": "1.1.8"
    }
}
